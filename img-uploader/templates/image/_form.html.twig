<div class="row" style="margin-bottom: 1.25rem;">
    <div class="col-sm-12">
        <div class="card">
            <div class="card-body" style="padding-bottom: 0;">
                {{ form_start(form) }}
                <div class="form-row">
                    <div class="form-group col-md-5">
                        {{ form_row(form.title) }}
                    </div>
                    <div class="form-group col-md-5">
                        {{ form_row(form.filename) }}
                    </div>
                    <div class="form-group col-md-2">
                        {{ form_row(form.save) }}
                    </div>
                </div>
                {{ form_end(form) }}
        </div>
    </div>
</div>
</div>



